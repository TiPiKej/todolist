{
  "name": "todolist",
  "version": "0.2",
  "homepage": "https://tipikej.github.io/todolist",
  "description": "Mini program to making 'to do' lists",
  "main": "index.js",
  "scripts": {
    "prestart": "start powershell npm run sass",
    "start": "npm run editor && npm run server",
    "server": "cd ./dist/ && live-server",
    "editor": "code .",
    "deploy": "gh-pages -d dist",
    "sass": "node-sass dist/sass --watch --recursive --output dist/css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TiPiKej/todolist.git"
  },
  "bugs": {
    "url": "https://github.com/TiPiKej/todolist/issues"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "live-server": "^1.2.0",
    "node-sass": "^4.10.0"
  }
}
